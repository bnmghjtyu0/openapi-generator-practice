{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.4.0",
    "generators": {
      "angular": {
        "generatorName": "typescript-angular",
        "output": "#{cwd}/openapi/angular",
        "glob": "api/v2.0/*.{json,yaml}",
        "additionalProperties": {
          "fileNaming": "kebab-case",
          "allowUnicodeIdentifiers":true,
          "modelFileSuffix":".model",
          "prependFormOrBodyParameters": true,
          "queryParamObjectFormat":"json",
          "serviceFileSuffix":".",
          "ngVersion": "12.0.1",
          "npmName": "restClient",
          "supportsES6": "true",
          "npmVersion": "6.9.0",
          "withInterfaces": true
        }
      },
      "java": {
        "generatorName": "java",
        "output": "#{cwd}/openapi/java",
        "glob": "api/v2.0/*.{json,yaml}"
      }
    }
  }
}
